<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" android:theme="@android:style/Theme.Translucent.NoTitleBar.Fullscreen" android:layout_gravity="center" android:id="@+id/inapp_interstitial_image_frame_layout" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:layout_gravity="center" android:id="@+id/interstitial_image_relative_layout" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="35dp">
        <ImageView android:id="@+id/interstitial_image" android:visibility="visible" android:layout_width="match_parent" android:layout_height="match_parent" android:scaleType="centerCrop" android:layout_alignParentTop="true" android:layout_centerHorizontal="true"/>
