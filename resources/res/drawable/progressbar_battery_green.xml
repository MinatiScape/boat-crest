<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/transparent"/>
        </shape>
    </item>
    <item android:id="@android:id/progress" android:left="4.5dp" android:top="4.5dp" android:right="4dp" android:bottom="4.5dp">
        <scale android:scaleWidth="100%">
            <shape>
                <corners android:radius="2dp"/>
                <gradient android:startColor="#03c28a" android:endColor="#03c28a" android:angle="90" android:type="linear"/>
            </shape>
        </scale>
    </item>
</layer-list>
